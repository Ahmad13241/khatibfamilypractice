{
  "name": "khatib-family-practice",
  "version": "1.0.0",
  "description": "Website for Khatib Family Practice with development logging capabilities",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "bs": "browser-sync start --config bs-config.js",
    "dev:sync": "concurrently \"npm run dev\" \"npm run bs\"",
    "lint": "eslint js/*.js",
    "logs:clear": "node scripts/clear-logs.js",
    "build": "node scripts/build.js",
    "start:prod": "NODE_ENV=production node server.js",
    "deploy:prepare": "npm run build && git add dist/* && git commit -m \"Update build for deployment\" && git push origin master",
    "fix:images": "node scripts/fix-image-refs.js"
  },
  "keywords": [
    "healthcare",
    "website",
    "logging",
    "debugging"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.10.0",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "browser-sync": "^2.29.3",
    "chalk": "^4.1.2",
    "clean-css": "5.3.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-promise": "^7.2.1",
    "glob": "^11.0.1",
    "nodemon": "^3.0.1",
    "terser": "^5.39.0"
  }
}
